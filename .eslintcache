}\n        });\n        setDisableButton(false);\n    };\n\n    const handleDeleteRuleClick = (element) => {\n        let updatedRules = data.group.rules.filter(rule => rule !== element);\n        setData({\n            ...data,\n            group: {\n                ...data.group,\n                rules: updatedRules\n            }\n        });\n        setDisableButton(false);\n    };\n\n    const handleDeleteObjectiveClick = (element) => {\n        let updatedObjectives = data.group.objectives.filter(rule => rule !== element);\n        setData({\n            ...data,\n            group: {\n                ...data.group,\n                objectives: updatedObjectives\n            }\n        });\n        setDisableButton(false);\n    };\n\n    const handleCheckUserGroupMembership = (userId, status) => {\n        props.dispatch(updateUserMembershipCreator(props.user.userData,\n            data.group.id,\n            {\n                targetUserId: userId,\n                status: status\n            }));\n        props.dispatch(getGroupCreator(props.user.userData, props.groupId));\n    };\n\n    const handleSaveClick = () => {\n        props.dispatch(updateOrCreateGroupCreator(props.user.userData, data.group));\n    };\n\n    return <Container className='container__main'>\n        <img\n            className='container__main-photo'\n            src={image}\n            alt=\"Group photo\"\n        />\n\n        <form className={classes.form}>\n            <Grid container style={{flexDirection: 'column'}} spacing={2}>\n                <Grid item xs={6}>\n                    <TextField\n                        id=\"name\"\n                        label=\"Nombre\"\n                        variant=\"outlined\"\n                        value={data.group.name}\n                        name='name'\n                        onChange={e => handleChangeTextFields(e)}\n                    />\n                </Grid>\n                <Grid item xs={6}>\n                    <TextField\n                        id=\"description\"\n                        label=\"Descripción\"\n                        variant=\"outlined\"\n                        value={data.group.description}\n                        name='description'\n                        onChange={e => handleChangeTextFields(e)}\n                    />\n                </Grid>\n            </Grid>\n\n\n            <ListSubheader\n                style={{marginTop: '20px'}}\n                component=\"div\" id=\"nested-list-subheader\">\n                Información adicional\n            </ListSubheader>\n\n\n            <Grid item xs={8}>\n                <CollapsableList\n                    typeName='Reglas'\n                    items={data.group.rules}\n                    onclick={handleDeleteRuleClick}/>\n            </Grid>\n            <Grid item xs={8}>\n                <CollapsableList\n                    typeName='Objetivos'\n                    items={data.group.objectives}\n                    onclick={handleDeleteObjectiveClick}/>\n            </Grid>\n\n            <Grid item xs={8}>\n                < ComplexCollapsableList\n                    typeName='Miembros activos'\n                    items={data.activeMembers}\n                    onclick={handleCheckUserGroupMembership}\n                />\n            </Grid>\n            <Grid item xs={8}>\n                < ComplexCollapsableList\n                    typeName='Solicitudes de ingreso'\n                    items={data.pendingMembers}\n                    showCheck={true}\n                    onclick={handleCheckUserGroupMembership}\n                />\n            </Grid>\n\n            <Grid\n                item xs={8}\n                style={{display: 'flex', justifyContent: 'flex-end'}}\n            >\n                <Button\n                    variant=\"contained\"\n                    color=\"primary\"\n                    className={classes.submit}\n                    onClick={handleSaveClick}\n                    disabled={disableButton}\n                >\n                    Guardar\n                </Button>\n            </Grid>\n\n        </form>\n    </Container>\n}\n\nconst mapStateToProps = state => {\n    return {\n        user: state.user,\n        group: state.group,\n    };\n};\n\nexport default connect(mapStateToProps)(GroupAdmin);","/home/ajardila/Desktop/usociety/src/components/SearchGroup/index.js",["100","101"],"/home/ajardila/Desktop/usociety/src/components/SingUp/SingUpUserData/index.js",["102"],"/home/ajardila/Desktop/usociety/src/components/hooks/useStyles.js",[],"/home/ajardila/Desktop/usociety/src/store/group/groupActions.js",[],"/home/ajardila/Desktop/usociety/src/store/group/groupReducer.js",[],"/home/ajardila/Desktop/usociety/src/components/CollapsableList/CollapsableList.js",[],"/home/ajardila/Desktop/usociety/src/components/CollapsableList/ComplexCollapsableList.js",[],"/home/ajardila/Desktop/usociety/src/store/group/groupInterfaces.js",[],"/home/ajardila/Desktop/usociety/src/store/user/userReducer.js",[],"/home/ajardila/Desktop/usociety/src/components/GroupPrincipal/Card2/index.js",["103","104"],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","severity":1,"message":"110","line":6,"column":5,"nodeType":"111","messageId":"112","endLine":6,"endColumn":7},{"ruleId":"109","severity":1,"message":"113","line":1,"column":8,"nodeType":"111","messageId":"112","endLine":1,"endColumn":13},{"ruleId":"109","severity":1,"message":"114","line":8,"column":9,"nodeType":"111","messageId":"112","endLine":8,"endColumn":16},{"ruleId":"109","severity":1,"message":"115","line":12,"column":18,"nodeType":"111","messageId":"112","endLine":12,"endColumn":25},{"ruleId":"109","severity":1,"message":"116","line":14,"column":8,"nodeType":"111","messageId":"112","endLine":14,"endColumn":20},{"ruleId":"109","severity":1,"message":"117","line":15,"column":8,"nodeType":"111","messageId":"112","endLine":15,"endColumn":17},{"rul